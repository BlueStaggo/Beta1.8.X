+
+			for (int xx = 0; xx < width; xx++) {
+				for (int zz = 0; zz < height; zz++) {
+					int color = 0;
+					int i = array[xx + zz * width];
+					if (i >= 0 && i < BiomeGenBase.field_35486_a.length) {

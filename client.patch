+	public static float chaotify(float x) {
+		float chaosFactor = chaos - 1.0F;
+		chaosFactor *= chaosFactor * Math.signum(chaosFactor);
+		chaosFactor = Math.min(Math.max(chaosFactor, 0.0F), 1.0F);
+		return x + (rand.nextFloat() - x) * chaosFactor;
+	}
